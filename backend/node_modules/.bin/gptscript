#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules/@gptscript-ai/gptscript/bin/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules/@gptscript-ai/gptscript/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules/@gptscript-ai/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules/@gptscript-ai/gptscript/bin/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules/@gptscript-ai/gptscript/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules/@gptscript-ai/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/@gptscript-ai+gptscript@0.9.5/node_modules:/home/cpow/Desktop/AI-Video-Generator/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../@gptscript-ai/gptscript/bin/gptscript"   "$@"
exit $?
